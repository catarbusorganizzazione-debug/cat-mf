{
  "name": "remote",
  "version": "0.0.57",
  "private": false,
  "scripts": {
    "commit": "cz",
    "ng": "ng",
    "prepare-start": "rimraf .husky && husky install && husky add .husky/pre-commit 'npx lint-staged'",
    "start": "ng serve --port 4204",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "release": "standard-version",
    "release:all": "npm run prettier && npm run test && sh server/release.js && npm run release",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,md,ts,html,component.html}\"",
    "install-deps": "npm i --legacy-peer-deps",
    "merge-all": "node ./scripts/merge-all.mjs",
    "lib-release": "node ./scripts/ui-kit-merge.mjs",
    "add-version": "node ./scripts/add-version.mjs"
  },
  "dependencies": {
    "@angular-architects/module-federation": "^13.1.0",
    "@angular/animations": "~13.3.1",
    "@angular/common": "~13.3.1",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "~13.3.1",
    "@angular/forms": "~13.3.1",
    "@angular/platform-browser": "~13.3.1",
    "@angular/platform-browser-dynamic": "~13.3.1",
    "@angular/router": "~13.3.1",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@ngrx/component-store": "^13.2.0",
    "@ngrx/effects": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-calendar": "^0.28.28",
    "bootstrap": "^4.5.0",
    "cors": "^2.8.5",
    "d3": "^7.8.4",
    "data-fns": "^0.1.8",
    "dayjs": "^1.10.6",
    "express": "^4.17.3",
    "file-saver": "^2.0.5",
    "husky": "^7.0.4",
    "immer": "^9.0.12",
    "ngx-build-plus": "^13.0.1",
    "ngx-mask": "~13.1.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~13.3.1",
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~13.3.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/cors": "^2.8.12",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^16.18.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "colors": "^1.4.0",
    "commitizen": "^4.2.4",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "editorconfig": "^1.0.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^7.2.0",
    "inquirer": "^9.2.9",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "ora": "^7.0.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "sass-resources-loader": "^2.2.5",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~4.6.3",
    "v4": "^0.0.1"
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "prebump": "if test -f package-lock.json; then mv package-lock.json package-lock-IGNORE.json; else exit 0; fi | bash",
      "posttag": "if test -f package-lock-IGNORE.json; then mv package-lock-IGNORE.json package-lock.json; else exit 0; fi | bash"
    }
  },
  "engines": {
    "node": "20.x"
  }
}
